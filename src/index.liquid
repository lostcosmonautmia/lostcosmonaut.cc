<!doctype html>
<html lang="en-US">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  {% capture page_title -%}{{ app.name }} â€” Musical Instrument Accessories{% endcapture -%}

  <title>{{ page_title }}</title>

  <link rel="stylesheet" href="/assets/stylesheets/application.css">

  <link rel="preload" href="/assets/stylesheets/application.css" as="style">

  <link rel="apple-touch-icon" href="/apple-touch-icon.png">
  <link rel="manifest" href="/manifest.webmanifest">
  <link rel="shortcut icon" href="/favicon.ico">

  <meta name="apple-mobile-web-app-title" content="{{ app.name }}">
  <meta name="application-name" content="{{ app.name }}">
  <meta name="description" content="{{ app.description }}">
  <meta name="theme-color" content="{{ app.theme_color }}">

  <meta property="og:author_name" content="{{ pkg.author.name }}">
  <meta property="og:author_url" content="{{ pkg.author.url }}">
  <meta property="og:description" content="{{ app.description }}">
  <meta property="og:image" content="{{ app.start_url }}{{ app.icons[2].src }}">
  <meta property="og:title" content="{{ page_title }}">
  <meta property="og:url" content="{{ app.start_url }}{{ page.url }}">
</head>
<body>

  <a href="/" rel="home" class="h-card">
    <img src="/assets/images/badge.png" alt="{{ app.name }}">
  </a>

</body>
</html>
